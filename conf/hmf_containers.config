/*
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    Config file with URIs to HMF Docker images
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    This config file is intended for rapidly testing updates to HMFTools for Hartwig
    pipeline v6.0.

    Once pipeline v6.0 is ready for a full release:
    - Rebuild the biocontainer images with the latest jar URIs
    - Replace the biocontainer URIs defined in each process
    - Delete this config file!!!

----------------------------------------------------------------------------------------
*/

process {

    withName: 'AMBER.*' {
        container = 'docker.io/hartwigmedicalfoundation/amber:4.1-rc.2'
    }

    withName: 'BAMTOOLS.*' {
        container = 'docker.io/hartwigmedicalfoundation/bam-tools:1.3-rc.1'
    }

    withName: 'CUPPA.*' {
        container = 'docker.io/hartwigmedicalfoundation/cuppa:2.3.0'
    }

    withName: 'CHORD.*' {
        container = 'docker.io/hartwigmedicalfoundation/chord:2.1-rc.1'
    }

    withName: 'COBALT.*' {
        container = 'docker.io/hartwigmedicalfoundation/cobalt:2.0-rc.1'
    }

    withName: 'ESVEE.*' {
        container = 'docker.io/hartwigmedicalfoundation/esvee:1.0-rc.4'
    }

    // Isofox java code has not been updated
    // withName: 'ISOFOX.*' {
    // 	container = ''
    // }

    // Lilac java code has not been updated
    // withName: 'LILAC.*' {
    // 	container = ''
    // }

    withName: 'LINX.*' {
        container = 'docker.io/hartwigmedicalfoundation/linx:2.0-rc.1'
    }

    withName: 'PURPLE.*' {
        container = 'docker.io/hartwigmedicalfoundation/purple:4.1-rc.1'
    }

    withName: 'REDUX.*' {
        container = 'docker.io/hartwigmedicalfoundation/redux:1.0-rc.1'
    }

    withName: 'SAGE.*' {
        container = 'docker.io/hartwigmedicalfoundation/sage:4.0-rc.2'
    }

    withName: 'PAVE.*' {
        container = 'docker.io/hartwigmedicalfoundation/pave:1.7-rc.1'
    }

    // Sigs java code has not been updated
    // withName: 'SIGS.*' {
    // 	container = ''
    // }

    withName: 'VIRUSINTERPRETER.*' {
        container = 'docker.io/hartwigmedicalfoundation/virus-interpreter:1.7-rc.2'
    }

    withName: 'ORANGE.*' {
        container = 'docker.io/hartwigmedicalfoundation/orange:3.7.1-rc.1'
    }

}


